<?xml version="1.0" encoding="utf-8" ?>
<Controls>
  <!-- содержит описание контрола AgreementListCustom14 для сервера версии 5.5, ResourceKey задает ключ файла ресурсов, в котором указано наименование контрола (либо задать атрибут DisplayName задает наименование контрола), которое будет отображаться в конструкторе разметок -->
  <Control Name="AgreementListCustom14" ControlGroupResourceKey="ControlGroup_Custom" DisplayName="AgreementListCustomVersion14" >
    <!-- содержит перечисление свойств контрола -->
    <Properties>
      <Property Type="Name" />
      <Property Type="Visibility" />
      <Property Type="Text" />
      <Property Type="StandardCssClass" />
      <Property Type="CustomCssClasses" />
    </Properties>
    <!-- содержит перечисление событий контрола -->
    <Events>
      <Event Name="Click" ResourceKey="ControlTypes_ClickEventProperty" />
    </Events>
  </Control>
</Controls>
